include {{ nginx_conf_path }}/keitaro/local/tracker.inc;
include {{ nginx_ip_list_cf_v4_path }};
real_ip_header X-Forwarded-For;

access_log {{ nginx_access_log_keitaro_tracker }} main {{ nginx_access_log_options }};
access_log {{ nginx_access_log_keitaro_tracker_status }} status {{ nginx_access_log_options }};

{% if php_engine == 'php-fpm' %}
include {{ nginx_conf_path }}/keitaro/tracker.engine.php-fpm.inc;
{% else %}
include {{ nginx_conf_path }}/keitaro/tracker.engine.roadrunner.inc;
{% endif %}
