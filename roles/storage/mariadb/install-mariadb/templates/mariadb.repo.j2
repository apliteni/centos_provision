# MariaDB CentOS {{ ansible_distribution_major_version|int }} repository list
# http://mariadb.org/mariadb/repositories/
[mariadb]
name = MariaDB
baseurl = http://ams2.mirrors.digitalocean.com/mariadb/yum/{{ mariadb_version }}/centos{{ ansible_distribution_major_version }}-amd64
module_hotfixes=1
gpgkey=http://ams2.mirrors.digitalocean.com/mariadb/yum/RPM-GPG-KEY-MariaDB
gpgcheck = 1
