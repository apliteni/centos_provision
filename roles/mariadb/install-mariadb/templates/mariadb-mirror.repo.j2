# MariaDB CentOS {{ ansible_distribution_major_version|int }} repository list
# http://mariadb.org/mariadb/repositories/
[mariadb-mirror]
name = MariaDB
baseurl = https://mirror.docker.ru/mariadb/yum/{{ mariadb_version }}/centos{{ ansible_distribution_major_version }}-amd64
module_hotfixes=1
gpgkey=https://mirror.docker.ru/mariadb/yum/RPM-GPG-KEY-MariaDB
gpgcheck=1
