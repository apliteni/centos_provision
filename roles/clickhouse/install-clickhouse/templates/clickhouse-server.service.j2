[Unit]
Description=LSB: Yandex clickhouse-server daemon
After=network.target

[Service]
Type=forking
Restart=no
TimeoutSec=5min
IgnoreSIGPIPE=no
KillMode=process
GuessMainPID=no
RemainAfterExit=yes
SuccessExitStatus=5 6
ExecStart=/etc/init.d/clickhouse-server start
ExecStop=/etc/init.d/clickhouse-server stop
