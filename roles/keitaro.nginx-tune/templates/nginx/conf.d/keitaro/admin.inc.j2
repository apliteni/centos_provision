include {{ nginx_conf_path }}/keitaro/local/admin.inc;

access_log {{ nginx_access_log_keitaro_admin }} main {{ nginx_access_log_options }};

fastcgi_split_path_info ^(.+\.php)(/.+)$;
fastcgi_pass unix:/var/run/{{ php_version }}-fpm-keitaro-nontracker.sock;
fastcgi_index index.php;
fastcgi_buffers 16 16k;
fastcgi_buffer_size 32k;
fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
include fastcgi_params;
